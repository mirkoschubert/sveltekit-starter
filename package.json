{
  "name": "sveltekit-starter",
  "version": "0.2.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "update": "npx npm-check -u",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0-next.64",
    "@sveltejs/adapter-vercel": "^1.0.0-next.66",
    "@sveltejs/kit": "^1.0.0-next.399",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.12",
    "cssnano-preset-default": "^5.2.12",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.7",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.54.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-feather-icons": "^4.0.0",
    "svelte-highlight": "^6.2.0",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "svelte-seo": "^1.4.1",
    "sveltekit-components": "^0.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/source-code-pro": "^4.5.10",
    "@fontsource/source-sans-pro": "^4.5.10"
  }
}
